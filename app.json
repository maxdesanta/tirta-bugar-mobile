{
  "expo": {
    "name": "tirta bugar",
    "slug": "tirta-bugar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161955"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#161955"
      },
      "fonts": [
        "./assets/fonts/Roboto-Regular.otf",
        "./assets/fonts/Roboto-Medium.otf",
        "./assets/fonts/Roboto-Black.otf",
        "./assets/fonts/Roboto-Bold.otf"
      ],
      "package": "com.expo.tirtabugar",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "294f13e4-954f-455b-948d-ae1a74937106"
      }
    }
  }
}
